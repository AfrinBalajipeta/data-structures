#include <stdio.h>
long long aVeryBigSum(int n, long long ar[]) {
    long long sum = 0;
    for (int i = 0; i < n; i++) {
        sum += ar[i];
    }
    return sum;
}
int main() {
    int n;
    scanf("%d", &n);
    long long ar[n];
    for (int i = 0; i < n; i++) {
        scanf("%lld", &ar[i]);
    }
    printf("%lld\n", aVeryBigSum(n, ar));
    return 0;
}
