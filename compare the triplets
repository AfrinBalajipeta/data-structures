#include <stdio.h>
#include <stdlib.h>
int* compareTriplets(int a[], int b[], int* result_size) {
    int* scores = malloc(2 * sizeof(int));
    scores[0] = 0; 
    scores[1] = 0; 
    *result_size = 2;
    for (int i = 0; i < 3; i++) {
        if (a[i] > b[i]) {
            scores[0]++;
        } else if (a[i] < b[i]) {
            scores[1]++;
        }
    }
    return scores;
}
int main() {
    int a[3], b[3];
    scanf("%d %d %d", &a[0], &a[1], &a[2]);
    scanf("%d %d %d", &b[0], &b[1], &b[2]);
    int result_count;
    int* result = compareTriplets(a, b, &result_count);
    printf("%d %d\n", result[0], result[1]);
    free(result);
    return 0;
}
