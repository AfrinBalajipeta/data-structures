#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#define MAX 5002
short L1[MAX][MAX];
short L2[MAX][MAX];
int main() {
    char A[MAX], B[MAX];
    if (scanf("%s %s", A, B) != 2) return 0;
    int n = strlen(A);
    int m = strlen(B);
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= m; j++) {
            if (i == 0 || j == 0) L1[i][j] = 0;
            else if (A[i - 1] == B[j - 1]) L1[i][j] = L1[i - 1][j - 1] + 1;
            else L1[i][j] = (L1[i - 1][j] > L1[i][j - 1]) ? L1[i - 1][j] : L1[i][j - 1];
        }
    }
    int original_lcs = L1[n][m];
    for (int i = n; i >= 0; i--) {
        for (int j = m; j >= 0; j--) {
            if (i == n || j == m) L2[i][j] = 0;
            else if (A[i] == B[j]) L2[i][j] = L2[i + 1][j + 1] + 1;
            else L2[i][j] = (L2[i + 1][j] > L2[i][j + 1]) ? L2[i + 1][j] : L2[i][j + 1];
        }
    }
    int total_ways = 0;
    for (int i = 0; i <= n; i++) {
        int seen[128] = {0};
        for (int j = 0; j < m; j++) {
            char c = B[j];
            if (!seen[(int)c]) {
                if (L1[i][j] + 1 + L2[i][j + 1] == original_lcs + 1) {
                    total_ways++;
                    seen[(int)c] = 1;
                }
            }
        }
    }
    printf("%d\n", total_ways);
    return 0;
}
